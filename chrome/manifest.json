{
    "manifest_version": 3,
    "name": "GIT by Native Current",
    "version": "0.0.1",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts": [],
    "permissions": [
        "activeTab",
        "storage",
        "unlimitedStorage",
        "downloads"
    ],
    "host_permissions": [
        "https://localhost:4200/*"
    ],
    "externally_connectable": {
        "matches": [
            "https://localhost:4200/*",
            "https://192.168.1.169:4200/*"
        ]
    },
    "web_accessible_resources": [
        {
            "resources": [
                "includes/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "action": {
        "default_title": "Start download (may take time)"
    }
}
